{
    "rust-analyzer.rustfmt.overrideCommand": [
        "leptosfmt",
        "--stdin",
        "--rustfmt"
    ],
    "editor.formatOnSave": true,
    "editor.rulers": [
        100
    ],
    "[rust]": {
        "editor.defaultFormatter": "rust-lang.rust-analyzer",
    },
    "files.associations": {
        "*.rs": "rust"
    },
}